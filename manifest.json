{
  "name": "Anitube tool",
  "version": "0.1",
  "manifest_version": 2,
  
  "description": "Anitubeをより快適に見るためのツール",

  "permissions": [
    "tabs", "http://*/*"
  ],

  "background": {
    "scripts": ["background.js"]
